<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.gxu.gxucpcsystem.Service.MysqlDao">

    <select id="queryAll" resultType="cn.edu.gxu.gxucpcsystem.domain.Admin">
        select
            d.userName ,
            d.password
        from user d
    </select>

    <insert id="addAdmin">
        insert into user (user.userName,user.password)
        values (#{userName},#{password})
    </insert>

    <delete id="delAdmin">
        delete into user (user.userName,user.password)
        values (#{userName},#{password})
    </delete>

</mapper>